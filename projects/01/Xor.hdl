/**
 * Exclusive-or gate:
 * out = (not(a) and b) or (a and not(b))
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in=a, out=nota);
    Not(in=b, out=notb);
    And(a=a, b=notb, out=c0);
    And(a=nota, b=b, out=c1);
    Or(a=c0, b=c1, out=out);
}